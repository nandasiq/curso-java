package controle;

public class For1 {

    public static void main(String[] args) {
        
        int i = 1;
        while(i <= 10) {
             System.out.printf("While: %d\n", i);
             i++;
        }
        
        for(x = 0; x <= 10; x++) {
            System.out.printf("for: %d\n", x);
        }
        
        
       
    }
}
