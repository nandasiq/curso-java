package classe;

public class ValorVsReferencia {
    
    public static void main(String[] args) {
        
        double a = 2;
        double b = a; // atribuicao por valor
        
        a++;
        b--;
        
        System.out.printf("Valor de a: %.1f.\nValor de b: %.1f.", a, b);
    }

}
