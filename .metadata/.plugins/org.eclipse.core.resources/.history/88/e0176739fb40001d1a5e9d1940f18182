package fundamentos;

public class DesafioAritmetico {

	public static void main(String[] args) {
		
	int a = 6 * (3 + 2);
	int b = (1 - 5) * ( 2 - 7 );

	a = a * a;
	b = b / 2;
	
	a = a / (3 * 2);
	b = b * b;
	
	a = a - b;
	
	a = a * a * a;
	
	a = a / (10 * 10 * 10);
	System.out.println(a);
	
	}
}
