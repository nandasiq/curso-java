package classe;

public class AreaCirTeste {
    
    public static void main(String[] args) {
        
        AreaCirc a = new AreaCirc(5.6);
        System.out.println(a.area());
        
        AreaCirc a1 = new AreaCirc(10);
        a1.PI = 5;
        System.out.println(a1.area());
        
        AreaCirc.PI = 5;
        System.out.println(AreaCirc.PI);
              
    }

}
